{
  "name": "react-report-builder",
  "version": "1.0.0",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "build": "webpack && gulp",
    "build:clean": "rimraf lib",
    "build:rebuild": "npm run build:clean && npm run build",
    "package:clean": "rimraf *.tgz",
    "package:create": "npm pack && renamer --find \"/react-report-builder-(.*).tgz/\" --replace react-report-builder.tgz *",
    "package:recreate": "npm run package:clean && npm run package:create",
    "rebuild-repack": "npm run build:rebuild && npm run package:recreate",
    "start": "npm run rebuild-repack && cd example && npm run reinstall:react-report-builder && npm start"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peekdata/react-report-builder.git"
  },
  "keywords": [
    "report",
    "builder",
    "react"
  ],
  "author": "Peekdata",
  "license": "LGPL-3.0-only",
  "bugs": {
    "url": "https://github.com/peekdata/react-report-builder/issues"
  },
  "homepage": "https://peekdata.io",
  "dependencies": {
    "bootstrap": "~3.3.7",
    "chart.js": "~2.8.0",
    "classnames": "~2.2.6",
    "isomorphic-fetch": "~2.2.1",
    "moment": "~2.24.0",
    "peekdata-datagateway-api-sdk": "~2.1.2",
    "react": "~16.8.6",
    "react-bootstrap": "~0.32.4",
    "react-chartjs-2": "~2.7.6",
    "react-copy-to-clipboard": "~5.0.1",
    "react-datepicker": "~1.8.0",
    "react-dom": "~16.8.6",
    "react-redux": "~5.1.1",
    "react-responsive-modal": "~3.6.0",
    "react-select": "~2.4.3",
    "react-sortable-hoc": "~0.6.8",
    "react-syntax-highlighter": "~10.2.1",
    "react-table": "~6.9.2",
    "redux": "~3.7.2",
    "redux-saga": "~0.16.2"
  },
  "devDependencies": {
    "@types/classnames": "2.2.7",
    "@types/node": "11.13.9",
    "@types/react": "16.8.15",
    "@types/react-dom": "16.8.4",
    "@types/react-redux": "7.0.8",
    "@types/react-select": "2.0.17",
    "@types/react-syntax-highlighter": "10.1.0",
    "awesome-typescript-loader": "5.2.1",
    "file-loader": "3.0.1",
    "gulp": "4.0.1",
    "gulp-clean-css": "4.2.0",
    "gulp-concat": "2.6.1",
    "gulp-sass": "4.0.2",
    "renamer": "1.1.1",
    "rimraf": "2.6.3",
    "source-map-loader": "0.2.4",
    "ts-loader": "6.0.0",
    "tslint": "5.16.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-lines-between-class-members": "1.3.1",
    "typescript": "3.4.5",
    "url-loader": "1.1.2",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.2"
  }
}